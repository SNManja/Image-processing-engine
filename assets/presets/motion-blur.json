{
  "name": "Motion blur and sharpen",
  "pipeline": [
    {
      "filter": "motionblur",
      "params": {
        "scale": 1.2
      }
    },
    {
      "filter": "linearAdjustment",
      "params": {
        "scale": 1.25,
        "offset": -35
      }
    },
    {
      "filter": "sharpen",
      "params": {
        "amount": 0.7,
        "border": "clamp"
      }
    }
  ],
  "statistics": {
    "histograms": {
      "blueChannel": false,
      "greenChannel": false,
      "redChannel": false
    }
  },
  "output_suffix": "_processed"
}
