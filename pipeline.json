{
  "name": "example_pipeline",
  "pipeline": [
    {
      "filter": "sepia"
    },
    {
      "filter": "linearAdjustment",
      "params": { "offset": 0, "scale": 1.0 }
    },
    {
      "filter": "alphaBlending",
      "params": { "alpha": [0.5, 1.0, 1.0] }
    }
  ],
  "statistics": {
    "histograms": {
      "greyscale": false
    }
  },
  "output_suffix": "_processed"
}