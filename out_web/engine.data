{
  "name": "Cool green-ish look",
  "pipeline": [
    
    {
      "filter": "bnw",
      "params": { "offset": 0, "scale": 1.0 }
    },
    {
      "filter":"alphaBlending",
      "params": {
        
        "alpha":[0.5,0.5,1.0]}
    }
    
  ],
  "statistics": {
    "histograms": {
      "greyscale": false
    }
  },
  "output_suffix": "_processed"
}{
  "name": "Dramatic black and white",
  "pipeline": [
    {
      "filter": "motionblur",
      "params": {
        "scale": 1.3,
        "border": "clamp"
      }
    },
    {
      "filter": "linearAdjustment",
      "params": {
        "scale": 1.3,
        "offset": -255
      }
    },
    {
      "filter": "bnw",
      "params": {}
    }
  ],
  "statistics": {
    "histograms": { "greyscale": true }
  },
  "output_suffix": "_processed"
}{
  "name": "Pencil sketch",
  "pipeline": [
    {
      "filter": "sobel",
      "params": {
        "greyscale": true      
      }
    },
    {
      "filter": "linearAdjustment",
      "params": {
        "scale": 2.0,
        "offset": -40
      }
    },
    {
      "filter": "invert"
    },
    {
      "filter": "blur",
      "params": {
        "size": 5
      }
    }
  ],
  "statistics": {
    "histograms": {
      "greyscale": false
    }
  },
  "output_suffix": "_processed"
}
{
  "name": "Motion blur and sharpen",
  "pipeline": [
    {
      "filter": "motionblur",
      "params": {
        "scale": 1.2
      }
    },
    {
      "filter": "linearAdjustment",
      "params": {
        "scale": 1.25,
        "offset": -35
      }
    },
    {
      "filter": "sharpen",
      "params": {
        "amount": 0.7,
        "border": "clamp"
      }
    }
  ],
  "statistics": {
    "histograms": {
      "blueChannel": false,
      "greenChannel": false,
      "redChannel": false
    }
  },
  "output_suffix": "_processed"
}
